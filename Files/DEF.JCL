//            DCB=(DSORG=PS,LRECL=1024,BLKSIZE=6144,RECFM=FB),
//            UNIT=SYSDA,SPACE=(CYL,(2,1)),DISP=(NEW,CATLG,DELETE),
//            VOL=SER=DBDRD2
